/* pages/wallet/index.wxss */
.container{
  width: 100%;
  height: 100vh;
  background-color:#f2f2f2;
}
.overage{
  padding: 40rpx;
  background-color: #fff;
  text-align: center;
}
.overage overage-head{
  font-size: 28rpx;
  padding-top: 20rpx;
}
.overage .overage-content{
  font-size:100rpx;
  font-weight: 900;
  padding: 30rpx 0;
  margin: 30rpx 0;
  display: block;
}
.overage .overage-desc{
border: 2rpx solid black;
border-radius: 60rpx;
padding: 10rpx 50rpx;
font-size: 28rpx;
}
.btn-charge{
  width: 90%;
  margin-top: 40rpx;
  background-color: #b9dd08;
}
.tapbar{
  padding: 40rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  margin-top:40rpx;
  align-items: center; 
}
.my-deposit{
  margin-top: 2rpx;
}
.color{
  color: #b9dd08;
  margin-right:20rpx; 
}